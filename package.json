{
  "name": "icourt-build-plugin",
  "version": "1.0.9",
  "description": "A vite build plugin",
  "main": "./dist/index.mjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "pnpm run build -- --watch --ignore-watch my-vue-app",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "acn:vue3": "pnpm run build && cd my-vue-app && pnpm run build",
    "prepublish": "pnpm run build",
    "release": "npx bumpp --push --tag --commit && pnpm publish"
  },
  "homepage": "https://github.com/qdleader/qdleader",
  "repository": {
    "type": "git",
    "url": "https://github.com/qdleader/qdleader"
  },
  "keywords": [
    "vite",
    "build",
    "plugin"
  ],
  "devDependencies": {
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs": "0.0.1-security",
    "prettier": "^2.7.1",
    "rd": "^2.0.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4",
    "vite": "*"
  },
  "author": "yyyang",
  "license": "ISC",
  "dependencies": {
    "path": "^0.12.7",
    "progress": "^2.0.3"
  }
}
